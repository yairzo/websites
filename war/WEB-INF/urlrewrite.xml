<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<!--

    Configuration file for UrlRewriteFilter
    http://www.tuckey.org/urlrewrite/

-->
<urlrewrite>

	
 	<rule>
        <from>^/$</from>
        <to last="true">/homePage.html</to>
    </rule>

	<rule>
        <from>^/([^\.]+)/$</from>
        <to last="true">/homePage.html?locale=$1</to>
    </rule>
    

	<rule>
        <from>^/(.*\.pdf)$</from>
        <to last="true">/fileViewer?textualPageFilename=$1&amp;contentType=pdf&amp;attachmentId=1</to>
    </rule>
    <rule>
        <from>^/call_for_proposal/([^\s]+)</from>
        <to last="true">/callForProposal.html?urlTitle=$1</to>
    </rule>
    <rule>
        <from>^/page/([^\s]+)</from>
        <to last="true">/textualPage.html?urlTitle=$1</to>
    </rule>
    <rule>
        <from>^/search/$</from>
        <to last="true">/search.html</to>
    </rule>
    <rule>
        <from>^/search/(.*)</from>
        <to last="true">/search.html?searchWords=$1</to>
    </rule>
    <rule>
        <from>^/search_funding/(.*)</from>
        <to last="true">/searchCallForProposal.html?searchWords=$1</to>
    </rule>
<!-- <rule>
        <from>^/searchCallForProposals.html$</from>
        <to last="true">/search_funding/</to>
    </rule>
-->
    
    
    






